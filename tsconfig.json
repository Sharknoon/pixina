{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "@intlify/vite-plugin-vue-i18n/client",
      "vite-plugin-pwa/client"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}